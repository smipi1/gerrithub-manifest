[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Pieter Smith (smipi1)
[access "refs/for/refs/*"]
	read = group user/Pieter Smith (smipi1)
	push = group user/Pieter Smith (smipi1)
	label-Code-Review = -2..+2 group user/Pieter Smith (smipi1)
	label-Verified = -1..+1 group user/Pieter Smith (smipi1)
	rebase = group user/Pieter Smith (smipi1)
	removeReviewer = group user/Pieter Smith (smipi1)
	submit = group user/Pieter Smith (smipi1)
[access "refs/heads/*"]
	read = group user/Pieter Smith (smipi1)
	create = group user/Pieter Smith (smipi1)
	push = +force group user/Pieter Smith (smipi1)
	pushMerge = group user/Pieter Smith (smipi1)
[access "refs/tags/*"]
	push = +force group user/Pieter Smith (smipi1)
	pushTag = group user/Pieter Smith (smipi1)
	pushSignedTag = group user/Pieter Smith (smipi1)
